package linefollowerbehaviours;
import lejos.nxt.*;
import lejos.robotics.subsumption.*;

public class LineFollowerArbitrator {
	public static void main(String args[]) {
		Behavior[] beh;
		beh = new Behavior[1];
		beh[0] = new FollowLine();
		
		Arbitrator arb = new Arbitrator(beh,true);
		arb.start();
	}
}

