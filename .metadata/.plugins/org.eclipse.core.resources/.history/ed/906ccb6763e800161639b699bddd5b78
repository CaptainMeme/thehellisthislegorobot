package legorobot;

import lejos.nxt.*;
import lejos.robotics.navigation.DifferentialPilot;

public class HelloWorld { 
    public static void main(String[] args) {
        DifferentialPilot pilot = new DifferentialPilot(56.0,26.0,Motor.B,Motor.C);
        pilot.forward();
        pilot.travel(300);
        pilot.rotate(180.0);
        pilot.travel(300);
        
        
        
    }
}
